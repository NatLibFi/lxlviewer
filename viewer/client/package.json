{
  "name": "viewer",
  "description": "viewer",
  "version": "0.3.0",
  "repository": ".",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "concat-cli": "^4.0.0",
    "kungbib-styles": "^1.2.4",
    "less": "~2.3.1",
    "less-plugin-clean-css": "^1.5.1",
    "natives": "~1.1.6",
    "parallelshell": "~1.0.4",
    "uglify-js": "^2.4.24"
  },
  "devDependencies": {
    "watch": "~0.13.0"
  },
  "scripts": {
    "postinstall": "mkdir -p static/build && npm run uglify && npm run app",
    "uglify": "uglifyjs static/js/*.js -c -m -o static/build/idkbse.min.js --source-map",
    "watch": "parallelshell 'npm run watch:id:less'",
    "watch:id:less": "watch 'npm run app:id:less' static/less/",
    "app": "parallelshell 'npm run app:id:less'",
    "app:id:less": "lessc --clean-css='--advanced' static/less/main_id.less static/build/id.css --source-map=static/build/id.css.map"
  }
}
