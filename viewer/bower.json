{
  "name": "viewer",
  "version": "0.0.0",
  "dependencies": {
    "bootstrap": "~3.3.7",
    "font-awesome": "~4.7.0",
    "d3": "~3.5.6",
    "jquery": "~3.1.0",
    "kungbib-styles": "kungbib/frontend-guide#1.2.1"
  },
  "install": {
    "base": "static/vendor",
    "path": {
      "js": "{name}/js",
      "css": "{name}/css",
      "map": "{name}/css",
      "less": "{name}/less",
      "eot": "{name}/fonts",
      "svg": "{name}/fonts",
      "otf": "{name}/fonts",
      "ttf": "{name}/fonts",
      "woff": "{name}/fonts",
      "woff2": "{name}/fonts"
    },
    "sources": {
      "kungbib-styles": [
        "bower_components/kungbib-styles/dist/css/**",
        "bower_components/kungbib-styles/dist/less/**"
      ],
      "bootstrap": [
        "bower_components/bootstrap/dist/css/bootstrap.css",
        "bower_components/bootstrap/dist/js/bootstrap.js",
        "bower_components/bootstrap/dist/fonts/**",
        "bower_components/bootstrap/less/**"
      ],
      "font-awesome": [
        "bower_components/font-awesome/css/font-awesome.min.css",
        "bower_components/font-awesome/fonts/**"
      ]
    }
  },
  "resolutions": {
    "jquery": "~3.1.0"
  }
}
